  <%= form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => {:class => "new_user form-signin", :role => "form"}) do |f| %>
  <h2 class="form-signin-heading">Sign in to <span class="moflix">moflix</span></h2>
  
  <%= f.email_field :email, :autofocus => true, :class => "form-control", :placeholder => "Email address", :required => "" %>

  <%= f.password_field :password, :class => "form-control", :placeholder => "Password", :required => "" %>

  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
  <% end -%>

  <div><%= f.submit "Sign in", :class => "btn btn-lg mf_btn btn-block" %></div>

  <%= render "devise/shared/links" %>
<% end %>






